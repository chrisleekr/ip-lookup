# Server Configuration
NODE_ENV=development
PORT=3000
HOST=localhost
LOG_LEVEL=info
BODY_LIMIT=1048576    # Request body size limit in bytes (1MB)
REQUEST_TIMEOUT=30000 # Global request timeout in milliseconds

# Swagger Configuration
SWAGGER_HOST=localhost:3000
SWAGGER_SCHEME=http

# Cache Configuration
CACHE_TTL=3600
CACHE_CHECK_PERIOD=600
CACHE_MAX_KEYS=10000

# IPinfo Configuration
IPINFO_API_TOKEN=your_ipinfo_api_token_here

# IP Lookup Controller Configuration
MAX_IPS_PER_REQUEST=100          # Maximum number of IPs allowed in a single request (1-1000)
REQUEST_TIMEOUT_MS=30000         # Request timeout in milliseconds (1-60000)
CACHE_CONTROL_MAX_AGE=3600       # Cache-Control max-age duration in seconds (0-86400)
CACHE_CONTROL_STALE_IF_ERROR=600 # Cache-Control stale-if-error duration in seconds (0-86400)
